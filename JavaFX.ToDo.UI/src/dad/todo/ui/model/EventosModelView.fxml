<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<fx:root hgap="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onMouseClicked="#onEventClicked" type="GridPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" />
      <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
      <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" />
      <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" />
      <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" />
      <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" />
      <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" />
      <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" />
      <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" />
   </columnConstraints>
   <children>
      <Label GridPane.columnIndex="1" />
      <Label fx:id="labelTitulo" text="Tarea x" GridPane.columnIndex="1" GridPane.valignment="CENTER" />
      <Label text="Desde" GridPane.columnIndex="2" GridPane.valignment="CENTER" />
      <Label text="Hasta" GridPane.columnIndex="4" GridPane.valignment="CENTER" />
      <Button fx:id="mapButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onMapButtonAction" style="-fx-background-color: transparent;" GridPane.columnIndex="6">
         <graphic>
            <ImageView fitHeight="29.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/Map%20Marker-48.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label fx:id="horaInicioLabel" text="23:00" GridPane.columnIndex="3" GridPane.valignment="CENTER" />
      <Label fx:id="horaFinLabel" text="24:00" GridPane.columnIndex="5" GridPane.valignment="CENTER" />
      <Button fx:id="editButton" mnemonicParsing="false" onAction="#onEditButtonAction" style="-fx-background-color: transparent;" GridPane.columnIndex="7">
         <graphic>
            <ImageView fitHeight="29.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/ic_edit_black_24dp_2x.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="eliminarButton" mnemonicParsing="false" onAction="#onEliminarButtonAction" style="-fx-background-color: transparent;" GridPane.columnIndex="8">
         <graphic>
            <ImageView fitHeight="42.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/ic_delete_forever_black_24dp_2x.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <CheckBox fx:id="checkBoxTerminado" mnemonicParsing="false">
         <GridPane.margin>
            <Insets left="15.0" />
         </GridPane.margin></CheckBox>
   </children>
</fx:root>
